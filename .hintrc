{
  "connector": {
    "name": "local",
    "options": {
      "pattern": ["**", "!.git/**", "!node_modules/**"]
    }
  },
  "extends": ["development"],
  "formatters": ["stylish"],
  "hints": [
    "axe/aria",
    "axe/color",
    "axe/forms",
    "axe/keyboard",
    "axe/language",
    "axe/name-role-value",
    "axe/parsing",
    "axe/semantics",
    "axe/sensory-and-visual-cues",
    "axe/structure",
    "axe/tables",
    "axe/text-alternatives",
    "axe/time-and-media",
    "meta-viewport",
    "disown-opener",
    "scoped-svg-styles",
    "button-type",
    "typescript-config/import-helpers",
    "typescript-config/is-valid",
    "typescript-config/no-comments",
    "typescript-config/strict",
    "typescript-config/target",
    "apple-touch-icons",
    "meta-charset-utf-8",
    "meta-theme-color"
  ]
}
