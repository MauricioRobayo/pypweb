{
  "name": "pypweb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint:eslint": "eslint .",
    "lint:stylelint": "stylelint \"**/*.{tsx,ts}\"",
    "lint": "run-p lint:*",
    "dev": "NODE_ICU_DATA=node_modules/full-icu next dev",
    "build": "NODE_ICU_DATA=node_modules/full-icu next build",
    "start": "next start",
    "stylelint-check": "stylelint-config-prettier-check",
    "format": "prettier --write .",
    "postinstall": "git config core.hooksPath .githooks || true",
    "prepare": "git config core.hooksPath .githooks || true",
    "test": "jest",
    "test:ci": "jest -ci"
  },
  "dependencies": {
    "@mauriciorobayo/pyptron": "^2.19.2",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "date-fns": "^2.23.0",
    "full-icu": "^1.3.4",
    "gray-matter": "^4.0.3",
    "next": "^11.0.1",
    "nprogress": "^0.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1",
    "styled-components": "^5.3.0",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@hint/hint-axe": "^4.4.4",
    "@hint/hint-meta-theme-color": "^4.0.6",
    "@types/classnames": "^2.3.0",
    "@types/gtag.js": "0.0.7",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.2",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.15",
    "@types/styled-components": "^5.1.11",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "axe-core": "^4.3.1",
    "babel-plugin-styled-components": "^1.13.2",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "hint": "^6.1.4",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "prettier-plugin-organize-imports": "^2.3.3",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^27.0.4",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/picoyplaca/pypweb.git"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "*.{tsx,ts}": "stylelint --fix",
    "*.{js,ts,tsx,md}": "prettier --write"
