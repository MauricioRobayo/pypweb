{
  "name": "pypweb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint:eslint": "next lint",
    "lint:stylelint": "stylelint \"**/*.{tsx,ts}\"",
    "lint": "run-p lint:*",
    "dev": "NODE_ICU_DATA=node_modules/full-icu next-remote-watch ./posts",
    "build": "NODE_ICU_DATA=node_modules/full-icu next build",
    "start": "next start",
    "stylelint-check": "stylelint-config-prettier-check",
    "format": "prettier --write .",
    "postinstall": "git config core.hooksPath .githooks || true",
    "prepare": "git config core.hooksPath .githooks || true",
    "test": "jest",
    "test:ci": "jest -ci",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@mauriciorobayo/pyptron": "^2.33.2",
    "@mauriciorobayo/react-tweet-embed": "^1.3.0",
    "classnames": "^2.3.1",
    "date-fns": "^2.24.0",
    "full-icu": "^1.3.4",
    "gray-matter": "^4.0.3",
    "next": "^11.1.2",
    "next-mdx-remote": "^3.0.4",
    "next-seo": "^4.28.1",
    "nextjs-google-analytics": "^1.0.1",
    "nextjs-progressbar": "^0.0.11",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "react-obfuscate-email": "^1.0.12",
    "react-tweet-embed": "^1.2.2",
    "styled-components": "^5.3.1",
    "styled-normalize": "^8.0.7",
    "svg-emojis": "^0.0.8",
    "switch-emoji": "^1.0.16"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@hint/hint-axe": "^4.4.4",
    "@hint/hint-meta-theme-color": "^4.0.6",
    "@types/classnames": "^2.3.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.6",
    "@types/react": "^17.0.24",
    "@types/styled-components": "^5.1.14",
    "axe-core": "^4.3.3",
    "axios": "^0.21.4",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "babel-plugin-styled-components": "^1.13.2",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "hint": "^6.1.4",
    "jest": "^27.2.1",
    "lint-staged": "^11.1.2",
    "next-remote-watch": "^1.0.0",
    "next-sitemap": "^1.6.168",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^27.0.5",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/picoyplaca/pypweb.git"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "*.{tsx,ts}": "stylelint --fix",
    "*.{js,ts,tsx,md}": "prettier --write"
  }
}
