{
  "name": "pypweb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint:eslint": "eslint .",
    "lint:stylelint": "stylelint \"**/*.scss\"",
    "lint:webhint": "hint . --telemetry=on",
    "lint": "run-p lint:*",
    "dev": "NODE_ICU_DATA=node_modules/full-icu next dev",
    "build": "NODE_ICU_DATA=node_modules/full-icu next build",
    "start": "next start",
    "stylelint-check": "stylelint-config-prettier-check",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@mauriciorobayo/pyptron": "^1.63.0",
    "@types/classnames": "^2.2.11",
    "axe-core": "^4.1.2",
    "classnames": "^2.2.6",
    "full-icu": "^1.3.1",
    "next": "^10.0.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sass": "^1.30.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@hint/hint-axe": "^4.4.3",
    "@hint/hint-meta-theme-color": "^4.0.5",
    "@types/gtag.js": "0.0.4",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "hint": "^6.1.3",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "prettier-plugin-organize-imports": "^1.1.1",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-csstree-validator": "^1.9.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/picoyplaca/pypweb.git"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "*.{scss}": "stylelint --fix",
    "*.{js,ts,tsx,scss,md}": "prettier --write"
  }
}
