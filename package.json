{
  "name": "pypweb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint:eslint": "eslint .",
    "lint:stylelint": "stylelint \"**/*.{tsx,ts}\"",
    "lint": "run-p lint:*",
    "dev": "NODE_ICU_DATA=node_modules/full-icu next dev",
    "build": "NODE_ICU_DATA=node_modules/full-icu next build",
    "start": "next start",
    "stylelint-check": "stylelint-config-prettier-check",
    "format": "prettier --write .",
    "postinstall": "git config core.hooksPath .githooks || true",
    "prepare": "git config core.hooksPath .githooks || true",
    "test": "jest",
    "test:ci": "jest -ci"
  },
  "dependencies": {
    "@mauriciorobayo/pyptron": "^2.17.1",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "date-fns": "^2.22.1",
    "full-icu": "^1.3.4",
    "gray-matter": "^4.0.3",
    "next": "^10.2.3",
    "nprogress": "^0.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1",
    "styled-components": "^5.3.0",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@hint/hint-axe": "^4.4.3",
    "@hint/hint-meta-theme-color": "^4.0.5",
    "@types/classnames": "^2.3.0",
    "@types/gtag.js": "0.0.5",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.8",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "axe-core": "^4.2.1",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "hint": "^6.1.3",
    "jest": "^27.0.1",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "prettier-plugin-organize-imports": "^2.1.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^27.0.1",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/picoyplaca/pypweb.git"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "*.{tsx,ts}": "stylelint --fix",
    "*.{js,ts,tsx,md}": "prettier --write"
